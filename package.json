{
  "name": "storybook-addon-module-mock",
  "version": "2.0.0",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./preset": {
      "require": "./dist/cjs/preset.js",
      "import": "./dist/esm/preset.js"
    },
    "./preview": {
      "require": "./dist/cjs/preview.js",
      "import": "./dist/esm/preview.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc -p ./tsconfig.esm.json && cpy esm dist",
    "watch": "tsc -b -w",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "cp": "pnpm build && cpy dist ../frontend/node_modules/storybook-addon-module-mock"
  },
  "dependencies": {
    "storybook": "^9.0.0",
    "@types/node": "22.14.0",
    "minimatch": "^10.0.1",
    "react-json-tree": "^0.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@storybook/react": "^9.0.0",
    "@types/react": "^19.1.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "react": "^19.1.0",
    "storybook": "^9.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "webpack": "^5.99.9"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  },
  "repository": "https://github.com/ReactLibraries/storybook-addon-module-mock",
  "keywords": [
    "storybook",
    "react",
    "test",
    "jest",
    "mock",
    "hook",
    "module",
    "import",
    "interactions"
  ],
  "author": "SoraKumo <info@croud.jp>"
}
